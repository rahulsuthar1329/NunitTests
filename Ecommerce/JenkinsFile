pipeline {
  agent any

  stages {
    stage('Build Code') {
      steps {
        script: // Use 'sh' only for complex shell commands
          sh """
          sudo apt install dotnet-sdk-8.0 -y
          dotnet restore .
          dotnet test -e "BrowserName=${browserName}"
          """
      }
    }
  }
}